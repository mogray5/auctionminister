	<queryString><![CDATA[SELECT e.FIRSTNAME, e.LASTNAME,
d.VENDORNAME, a.PONUMBER, a.PODATE,
b.ITEMID, b.PURCHPRICE, b.QTYPURCH, 
(b.PURCHPRICE * b.QTYPURCH) as PURCHTOTAL
FROM po300 c inner join po301 a 
on c.DOCID = a.DOCID
inner join po302 b 
on a.PONUMBER = b.PONUMBER
inner join vn001 d
on a.VENDORID = d.VENDORID
inner join sy001 e 
on c.USERID = e.USERID
WHERE c.USERID= $P{pUser} AND a.PODATE >= $P{pStartDate} AND
a.PODATE <= $P{pEndDate}
ORDER BY d.VENDORNAME]]></queryString>
